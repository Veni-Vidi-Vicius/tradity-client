{
  "name": "tradity-client",
  "version": "0.7.0",
  "devDependencies": {
    "angular-gettext-cli": "^1.0.7",
    "angular-write-constants": "^1.0.2",
    "autoprefixer": "^6.3.1",
    "browser-sync": "^2.11.1",
    "connect-history-api-fallback": "^1.1.0",
    "cssmin": "^0.4.3",
    "hashmark": "^3.1.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "~0.1.3",
    "less": "^2.5.3",
    "ngdoc-md": "^1.0.3",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.4.1",
    "remarkup-po": "^0.1.2",
    "requirejs": "^2.1.11",
    "rimraf": "^2.5.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "watch-run": "^1.2.4"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "lint": "jshint --config=.jshintrc --reporter=node_modules/jshint-stylish app/js/*.js app/js/*/*.js",
    "test": "npm run lint",
    "gettext-extract": "angular-gettext-cli --files '{app/**/*.html,app/js/**/*.js}' --dest po/unstripped/templates.pot",
    "gettext-compile": "angular-gettext-cli --compile --files po/unstripped/de.po --dest dist/app/js/jit/l10n/de.json --module tradity --format json",
    "unmarkup": "remarkup-po unmarkup po/unstripped/templates.pot > po/templates.pot",
    "remarkup": "remarkup-po remarkup po/unstripped/templates.pot po/de.po > po/unstripped/de.po",
    "translate-extract": "npm run gettext-extract && npm run unmarkup",
    "translate-build": "npm run remarkup && npm run gettext-compile",
    "translate-cycle": "npm run translate-extract && npm run translate-build",
    "clean": "rimraf dist/ docs/ && mkdir -p docs/ dist/app/js/jit/l10n/",
    "docs": "ngdoc-md 'app/js/**/*.js' docs/",
    "prebuild": "npm run clean",
    "build": "parallelshell 'npm run build:css' 'npm run build:js' 'npm run build:cp' 'npm run translate-cycle'",
    "build:css": "lessc less/main.less | postcss --use autoprefixer --autoprefixer.browsers 'last 1 version' | cssmin > dist/main.css",
    "build:js": "parallelshell 'npm run build:js:copy' 'npm run build:js:stamp' 'npm run build:js:config'",
    "build:js:copy": "cp -r app dist/ && (cd dist && ln -sf ../node_modules || true)",
    "build:js:config": "angular-write-constants --module tradity app/js/config.global.json app/js/config.local.json > dist/app/js/config.js",
    "build:js:stamp": "echo \"var TRADITY_BUILD_STAMP = 'TDYC$(date +%s)-$(git rev-parse HEAD)';\" > dist/app/js/buildstamp.js",
    "build:cp": "cp -r *.html *.txt *.ico fonts img scripts apidoc dist/",
    "watch": "parallelshell 'npm run watch:css' 'npm run watch:js' 'npm run watch:cp'",
    "watch:css": "watch-run -p 'less/**' npm run build:css",
    "watch:js": "watch-run -p 'app/**' npm run build:js",
    "watch:cp": "watch-run -p '*.html,*.txt,*.ico,fonts/**,img/**,scripts/**' npm run build:cp",
    "start": "npm run build && parallelshell 'npm run watch' 'browser-sync start --config bs-config.js'",
    "typings": "typings",
    "tsc": "tsc -p . -w"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular2-material/button": "^2.0.0-alpha.4",
    "@angular2-material/core": "^2.0.0-alpha.4",
    "@angular2-material/input": "^2.0.0-alpha.4",
    "@angular2-material/sidenav": "^2.0.0-alpha.4",
    "@angular2-material/toolbar": "^2.0.0-alpha.4",
    "angular": "1.5.3",
    "angular-debounce": "~1.0.1",
    "angular-dialog-service": "m-e-conroy/angular-dialog-service#b6471bb",
    "angular-events-calendar": "^1.0.0",
    "angular-gettext": "addaleax/angular-gettext#development-all",
    "angular-marked": "^1.0.1",
    "angular-md5": "*",
    "angular-sanitize": "1.5.3",
    "angular-ui-bootstrap": "0.14.x",
    "angular-ui-router": "~0.2.14",
    "asmcrypto.js": "vibornoff/asmcrypto.js#v0.0.6",
    "bootstrap": "3.3.x",
    "clipboard": "~1.5.3",
    "connect-modrewrite": "^0.7.5",
    "debug": "nhaesler/debug#100cf7e",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "font-awesome": "4.5.0",
    "jquery": "2.1.1",
    "lzma": "addaleax/LZMA-JS#development-all",
    "munkres-js": "^1.1.3",
    "ng-infinite-scroll": "1.2.x",
    "nsPopover": "nohros/nsPopover#622a133b344b",
    "open-sans-fontface": "~1.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.6",
    "systemjs": "^0.19.27",
    "zone.js": "^0.6.12",
    "zxcvbn": "^4.2.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tradity/tradity-client.git"
  },
  "bugs": {
    "url": "https://github.com/tradity/tradity-client/issues"
  }
}
